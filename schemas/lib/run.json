{
	"$schema": "http://json-schema.org/draft-04/schema",

	"type": "object",
	"additionalProperties": false,
	"properties": {
		"pk": {"type": "number"},
		"index": {"type": "number"},
		"scheduled": {"type": "number"},
		"title": {"type": "string"},
		"category": {"type": "string"},
		"platform": {"type": "string"},
		"duration": {"type": "string"},
		"runners": {
			"type": "array",
			"items": {"$ref": "participant.json"}
		},
		"commentators": {
			"type": "array",
			"items": {"$ref": "participant.json"}
		}
	},
	"required": [
		"pk",
		"index",
		"scheduled",
		"title",
		"duration",
		"runners",
		"commentators"
	]
}
