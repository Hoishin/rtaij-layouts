{
	"$schema": "http://json-schema.org/draft-04/schema",

	"type": "array",
	"items": {
		"type": "object",
		"additionalProperties": false,
		"properties": {
			"input": {
				"oneOf": [{"type": "null"}, {"type": "string"}],
				"default": null
			},
			"viewId": {"type": "string", "default": ""}
		},
		"required": ["input", "viewId"]
	},
	"minItems": 4,
	"maxItems": 4
}
