{
  "name": "rtainjapan",
  "version": "0.0.0",
  "description": "The on-stream layout for RTA in Japan speedrunning marathon event",
  "homepage": "https://rtainjapan.net/",
  "author": {
    "name": "Hoishin",
    "email": "hoishinxii@gmail.com",
    "url": "https://twitter.com/hoishinxii"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension"
  ],
  "nodecg": {
    "compatibleRange": "^0.9.8",
    "dashboardPanels": [
      {
        "name": "stream",
        "title": "配信",
        "file": "stream-tech.html",
        "headerColor": "#9f9bbd",
        "fullbleed": true
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720
      }
    ]
  },
  "xo": {
    "semicolon": true,
    "rules": {
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "nodecg.Replicant"
          ]
        }
      ]
    }
  },
  "scripts": {
    "lint": "node_modules/.bin/xo --fix"
  },
  "dependencies": {
    "clone": "^2.1.1",
    "superagent": "^3.8.0",
    "twemoji": "^2.5.0",
    "twitter-stream-api": "^0.5.2"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "keywords": [
    "nodecg-bundle"
  ],
  "license": "MIT"
}
