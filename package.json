{
	"name": "rtainjapan-layouts",
	"private": true,
	"scripts": {
		"start": "(cd ../.. && node index.js)",
		"serve:ex": "parcel watch src/extension/index.ts --target node --out-dir extension --public-url '.'",
		"build:ex": "parcel build src/extension/index.ts --target node --out-dir extension --public-url '.'",
		"serve:db": "parcel watch src/dashboard/tech.html --out-dir dashboard --public-url '.'",
		"build:db": "parcel build src/dashboard/tech.html --out-dir dashboard --public-url '.'",
		"serve:tw": "parcel watch src/twitter-callback/index.html --out-dir twitter-callback --public-url '.'",
		"build:tw": "parcel build src/twitter-callback/index.html --out-dir twitter-callback --public-url '.'",
		"build:schema": "ts-node schema-types.ts",
		"format": "prettier '{src,types,schemas}/**/*.{ts,tsx,js,jsx,json,scss}' --write"
	},
	"dependencies": {
		"@material-ui/core": "^1.4.0",
