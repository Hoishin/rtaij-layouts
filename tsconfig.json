{
	"compilerOptions": {
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,

		"target": "esnext",
		"lib": ["esnext", "dom"],
		"jsx": "react",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"importHelpers": true
	}
}
