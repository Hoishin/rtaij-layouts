{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"lib": ["esnext", "dom"],
		"outDir": ".",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"jsx": "react"
	},
	"include": ["src"],
	"exclude": []
}
