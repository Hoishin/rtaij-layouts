{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"lib": ["esnext"],
		"outDir": "./extension",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true
	},
	"include": [
		"src/extension"
	],
	"exclude": []
}
