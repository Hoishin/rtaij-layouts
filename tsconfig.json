{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"lib": ["esnext", "dom"],
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"jsx": "react"
	}
}
