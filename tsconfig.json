{
	"compilerOptions": {
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,

		"target": "es2017",
		"lib": ["esnext", "dom"],
		"jsx": "react",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	}
}
